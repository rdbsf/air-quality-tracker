#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/bin/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules:/Users/ryanbishop/jsframeworks/heroku/node_modules:/Users/ryanbishop/jsframeworks/node_modules:/Users/ryanbishop/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../sshpk/1.15.2/node_modules/sshpk/bin/sshpk-sign" "$@"
  ret=$?
else 
  NODE_PATH="/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/bin/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules/sshpk/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/1.15.2/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/sshpk/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules/.registry.npmjs.org/node_modules:/Users/ryanbishop/jsframeworks/heroku/airquality/node_modules:/Users/ryanbishop/jsframeworks/heroku/node_modules:/Users/ryanbishop/jsframeworks/node_modules:/Users/ryanbishop/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../sshpk/1.15.2/node_modules/sshpk/bin/sshpk-sign" "$@"
  ret=$?
fi
exit $ret
